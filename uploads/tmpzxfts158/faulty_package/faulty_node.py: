#!/usr/bin/env python3

# Missing proper imports - ERROR
import rclpy
from std_msgs.msg import Float64
# Missing sensor_msgs import

class FaultyNode:
    # Missing Node inheritance - ERROR
    # Missing rclpy.init - ERROR
    
    def __init__(self):
        # Publisher without proper parameters - STYLE ERROR
        self.pub=rclpy.create_publisher(Float64,'command')  # No queue_size, no spaces
        
        # No rate defined - SAFETY ISSUE
    
    def move_joints(self):
        """Move with unsafe values"""
        # Unsafe joint values (way too large) - SAFETY ERROR
        unsafe_joints = [10.0, 15.0, 20.0, 25.0, 30.0, 35.0]
        
        # Infinite loop without sleep - SAFETY ERROR
        while True:
            for joint in unsafe_joints:
                msg=Float64()  # Style issue
                msg.data=joint
                self.pub.publish(msg)
            # NO SLEEP/RATE - Will consume 100% CPU!

def main():
    # Missing rclpy.init() - ERROR
    node=FaultyNode()  # Style issue - no spaces
    node.move_joints()

if __name__=='__main__':  # Style issue - no spaces
    main()
